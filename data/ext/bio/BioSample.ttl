@prefix schema: <http://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

schema:BioSample
  a rdfs:Class ;
  schema:isPartOf <http://bio.schema.org> ;
  rdfs:label "BioSample" ;
  rdfs:comment """A biological material entity that is representative of a whole.

    Comments: Typically samples are intended to be representative of the whole or aspects thereof. Examples of samples include biomedical samples (blood, urine) and plant specimens held at herbaria.""" ;
  rdfs:subClassOf schema:BioChemEntity ;
  dc:source <http://bioschemas.org> .

schema:additionalProperty
  a rdf:Property ;
  schema:domainIncludes schema:BioSample .

schema:collector
  a rdf:Property ;
  schema:isPartOf <http://bio.schema.org> ;
  rdfs:label "collector" ;
  rdfs:comment "The Person or Organization who collected the Sample." ;
  schema:domainIncludes schema:BioSample ;
  schema:rangeIncludes schema:Person, schema:Organization .

schema:custodian
  a rdf:Property ;
  schema:isPartOf <http://bio.schema.org> ;
  rdfs:label "custodian" ;
  rdfs:comment "The Person or Organization who is responsible for the Sample." ;
  schema:domainIncludes schema:BioSample ;
  schema:rangeIncludes schema:Person, schema:Organization .

schema:dateCreated
  a rdf:Property ;
  schema:domainIncludes schema:BioSample .

schema:itemLocation
  a rdf:Property ;
  schema:domainIncludes schema:BioSample .

schema:locationCreated
  a rdf:Property ;
  schema:domainIncludes schema:BioSample .

schema:samplingAge
  a rdf:Property ;
  schema:isPartOf <http://bio.schema.org> ;
  rdfs:label "samplingAge" ;
  rdfs:comment "The age of the object when the Sample was created." ;
  schema:domainIncludes schema:BioSample ;
  schema:rangeIncludes schema:Integer .

schema:gender
  a rdf:Property ;
  schema:domainIncludes schema:BioSample .

schema:isControl
  a rdf:Property ;
  schema:isPartOf <http://bio.schema.org> ;
  rdfs:label "isControl" ;
  rdfs:comment "Indicates whether the sample is being used as a normal control, may be in combination with another sample." ;
  schema:domainIncludes schema:BioSample ;
  schema:rangeIncludes schema:Boolean .
